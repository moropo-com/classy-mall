{
  "expo": {
    "name": "Shopper Map",
    "description": "Shopping centre navigation",
    "slug": "shoppermap",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "primaryColor": "#fff",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./img/square.png",
    "splash": {
      "image": "./img/launch.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "org.shoppermap.exp",
      "versionCode": 1,
      "permissions": [],
      "icon": "./img/square.png"
    },
    "ios": {
      "bundleIdentifier": "org.shoppermap.exp",
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true
    }
  }
}
